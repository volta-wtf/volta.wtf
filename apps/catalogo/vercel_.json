{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "installCommand": "cd ../.. && pnpm install",
  "buildCommand": "cd ../.. && pnpm -F apps/catalogo build",
  "devCommand": "cd ../.. && pnpm -F apps/catalogo dev",
  "framework": "nextjs",
  "outputDirectory": "apps/catalogo/.next",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}